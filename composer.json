{
    "name": "l6-hosting/perpendicular",
    "description": "PHP Library that communicates with Parallel's PBAS SDK",
    "authors": [
        {
            "name": "Travis Beck",
            "email": "tjbenator@gmail.com"
        }
    ],
    "require": {
        "nusphere/nusoap": "0.9.5"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "nusphere/nusoap",
                "version": "0.9.5",
                "dist": {
                    "url": "http://downloads.sourceforge.net/project/nusoap/nusoap/0.9.5/nusoap-0.9.5.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["lib/"]
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "L6Hosting\\Perpendicular\\": "src/"
        }
    }

}
